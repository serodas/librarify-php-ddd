<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="MyLibrary\Backoffice\Books\Domain\BackofficeBook" table="backoffice_books">
        <id name="id" type="string" column="id" length="36" />
        <id name="title" type="string" column="name" length="255" />
        <id name="description" type="string" column="duration" length="255" />
        <id name="score" type="integer" column="score" />
    </entity>
</doctrine-mapping>
